<Presentation PresentationClass="profile">
  <PageOptions Columns="3"/>
  <WindowName>Find/collaborate with leading health researchers</WindowName>
  <PageTitle>Search</PageTitle>
  <PageBackLinkName></PageBackLinkName>
  <PageBackLinkURL></PageBackLinkURL>
  <PageSubTitle></PageSubTitle>
  <PageDescription></PageDescription>
  <PanelList>
    <Panel Type="header"  />
    <Panel Type="page" />
    <Panel Type="active">
      <Module ID="MainMenu">
      </Module>
      <Module ID="Gadgets" >
        <ParamList>
          <Param Name="GadgetDiv">gadgets-tools-left</Param>
          <Param Name="GadgetClass">gadgets-gadget-parent</Param>
        </ParamList>
      </Module>
    </Panel>
    <Panel Name="Find People" Alias="people" Type="main" TabType="Default" TabSort="0">
      <Module ID="SearchPerson">
      </Module>    
    </Panel>
    <Panel Name="Find Research" Alias="all" Type="main" TabType="Fixed" TabSort="1" >
      <Module ID="SearchEverything" />        
    </Panel>
    <Panel Type="passive" >
      <Module ID="TopSearchPhrase">
        <ParamList>
          <Param Name="Description">Most viewed</Param>
          <Param Name="TimePeriod">d</Param>
        </ParamList>
      </Module>
      <!--Module ID="TopSearchPhrase">
        <ParamList>
          <Param Name="Description">Most viewed (month)</Param>
          <Param Name="TimePeriod">m</Param>
        </ParamList>
      </Module-->
    </Panel>
    <Panel type="footer" />
  </PanelList>
</Presentation>

